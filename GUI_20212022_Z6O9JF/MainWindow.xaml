<Window x:Class="GUI_20212022_Z6O9JF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI_20212022_Z6O9JF"
        xmlns:vm="clr-namespace:GUI_20212022_Z6O9JF.ViewModels"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        mc:Ignorable="d"
        Title="RTSDEV" Height="1000" Width="1800" WindowState="Maximized" WindowStyle="None" Icon="Images\Other\icon.png">
    <Grid Name="grid">
        <Image gif:ImageBehavior.AnimatedSource="\Images\Menu\main_background.gif" Stretch="Fill" Grid.ColumnSpan="1" Grid.RowSpan="1"/>
        <ContentControl Content="{Binding View}"></ContentControl>
        <MediaElement Source="Music/standard.mp3" LoadedBehavior="Manual" Name="music"/>
        <Button x:Name="btn_mute" Background="Transparent"  VerticalAlignment="Bottom" HorizontalAlignment="Left" Click="Button_Click" Height="90" Width="90" Margin="30,0,0,30" BorderBrush="Transparent">
        </Button>
    </Grid>
</Window>
